{
  "name": "ametrine.host",
  "version": "1.5.0",
  "description": "Hosting service providing various virtual hosting solutions at an affordable price.",
  "main": "./dist/main.js",
  "scripts": {
    "lint": "npx eslint ./src --ext .ts",
    "build": "cp -r src/* dist&&echo '[ametrine] building'&&npx tsc -p .",
    "sql": "echo '[ametrine] merging sql files'&&sqlite3 data/db/database.db < dist/sql/init.sql",
    "start": "pm2 start dist/main.js",
    "nstart": "cd dist&&node main.js&&cd ..",
    "mb": "echo '[ametrine] moving billing from dist to src'&&rm -rf src/modules/views/billing&&mkdir src/modules/views/billing&&cp -r dist/modules/views/billing/* src/modules/views/billing",
    "ls": "npm run build && npm run nstart"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Em1tt/ametrine.host.git"
  },
  "author": "Ametrine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Em1tt/ametrine.host/issues"
  },
  "homepage": "https://github.com/Em1tt/ametrine.host#readme",
  "dependencies": {
    "argon2": "^0.28.3",
    "body-parser": "^1.19.0",
    "canvas": "^2.8.0",
    "cookie-parser": "^1.4.5",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "eta": "^1.12.3",
    "express": "^4.17.2",
    "express-rate-limit": "^6.0.4",
    "hastily": "^0.5.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mailparser": "^3.4.0",
    "mime": "^3.0.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.2",
    "multer": "^1.4.4",
    "node-2fa": "^2.0.3",
    "node-fetch": "^2.6.5",
    "nodemailer": "^6.6.3",
    "qrcode": "^1.5.0",
    "redis": "^3.1.2",
    "sharp": "^0.29.3",
    "smtp-server": "^3.9.0",
    "stripe": "^8.182.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.6.0",
    "typescript": "^4.5.4"
  }
}
